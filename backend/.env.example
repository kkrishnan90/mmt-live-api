# Gemini API Configuration
# GEMINI_API_KEY=your_gemini_api_key_here

# Vertex AI Configuration
# Set to 'true' to use Vertex AI with service account authentication
# Set to 'false' to use regular Google AI API with API key authentication
GOOGLE_GENAI_USE_VERTEXAI=true

# Google Cloud Project Configuration (required when using Vertex AI)
# Your Google Cloud Project ID where Vertex AI is enabled
GOOGLE_CLOUD_PROJECT_ID=your-project-id

# Google Cloud Location/Region (required when using Vertex AI)
# Common regions: us-central1, us-east1, europe-west1, asia-northeast1
GOOGLE_CLOUD_LOCATION=us-central1

# Gemini Model Configuration
# Model name for Gemini Live API - use the live model for voice capabilities
GEMINI_MODEL_NAME=gemini-live-2.5-flash

# Optional: Alternative API key environment variable name (when not using Vertex AI)
# GOOGLE_API_KEY=your_gemini_api_key_here